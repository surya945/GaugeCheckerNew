<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suryajeet945.cc.gaugematchernew">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowRecords">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suryajeet945.cc.gaugematchernew.MainActivity" />
        </activity>
        <activity android:name=".ShowResult" />
        <activity android:name=".TakeWiresSizes">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suryajeet945.cc.gaugematchernew.MainActivity" />
        </activity>
        <activity android:name=".WireMatcher">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suryajeet945.cc.gaugematchernew.TakeWiresSizes" />
        </activity>
        <activity android:name=".HowToUse">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suryajeet945.cc.gaugematchernew.MainActivity" />
        </activity>
        <activity android:name=".SettingActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suryajeet945.cc.gaugematchernew.MainActivity" />
        </activity>
        <activity android:name=".GaugeMeasureActivity"></activity>
    </application>

</manifest>